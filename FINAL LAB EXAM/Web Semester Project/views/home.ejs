      <h1 class="display-4 fst-italic title">Something to be <br>ramenbered.</h1>
    </div>
  </div>







  <div class="album py-5 bg-body-tertiary">
    <div class="container">
      <h2 class="pb-2 border-bottom">Menu Items</h2>

      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
        <% for( let index = 0; index < fooditems.length; index++ ) { %>
        <div class="col">
          <div class="card shadow-sm">
            <img src="/pictures/katsu.jpeg" >
            <div class="card-body">
              <h2><%= fooditems[index].name %></h2>
              <p class="card-text"><%= fooditems[index].price %></p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                  <button type="button" class="btn btn-sm btn-outline-secondary">View Details</button>
                  <a href="/add-cart/<%=fooditems[index]._id %>">
                  <button type="button" class="btn btn-sm btn-outline-secondary">Add to Order</button>
                 </a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% } %>

        
        
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


